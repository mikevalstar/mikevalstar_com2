<!doctype html>
<html lang="en">
<head>
    {{>head}}
</head>
<body>
    {{>nav}}
    <div id="content">
        <article>
            <ul>
                {{#each collections.blog}}
                    <li>
                        <h3><a href="{{this.path}}">{{this.title}}</a></h3>
                        <article>{{this.short}}</article>
                    </li>
                {{/each}}
            </ul>
        </article>
    </div>
</body>
</html>
